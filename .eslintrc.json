{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [2, "double", {"avoidEscape": true}],
        "indent": ["error", 4]
    }
}
